{
    "name": "minesweeper",
    "version": "1.0.0",
    "description": "A minesweeper clone",
    "scripts": {
        "install:client": "cd src/client && npm install",
        "install:server": "cd src && npm install",
        "build:client": "cd src/client && rm -rf public && npm run build",
        "prod": "npm run install:client && npm run install:server && npm run build:client && cd src && npm run prod"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/marianafcosta/minesweeper.git"
    },
    "author": "Mariana Costa",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/marianafcosta/minesweeper/issues"
    },
    "homepage": "https://github.com/marianafcosta/minesweeper#readme"
}
